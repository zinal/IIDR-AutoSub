<?xml version="1.0" encoding="UTF-8"?>
<idrcdc-autosub>
    <idrcdc-subscription name='O2K1' source='WRK1' target='KAFKA1' />
    <idrcdc-subscription name='O2K2' source='WRK1' target='KAFKA1' />
    <idrcdc-subscription name='O2M1' source='WRK1' target='META' skipNewBlobs='true' />
    <idrcdc-subscription name='O2M2' source='WRK1' target='META' skipNewBlobs='true' />
    <idrcdc-subscription name='O2D1' source='WRK1' target='DS' />
    <!--
    <idrcdc-subscription name='P2O1' source='PG1' target='META' skipNewBlobs='true' />
    -->
    <idrcdc-engine name='WRK1' type='Source'>
        <cdc-rsh>ssh cdcuser@zodak</cdc-rsh>
        <cdc-path>/datum/sw/cdc-oracle</cdc-path>
        <cdc-instance>wrk1</cdc-instance>
    </idrcdc-engine>
    <idrcdc-engine name='PG1' type='Source'>
        <cdc-rsh>ssh cdcuser@kafko</cdc-rsh>
        <cdc-path>/home/cdcuser/cdc-postgres</cdc-path>
        <cdc-instance>pg1</cdc-instance>
    </idrcdc-engine>
    <idrcdc-engine name='KAFKA1' type='Target'>
        <cdc-rsh>ssh cdcuser@kafko</cdc-rsh>
        <cdc-path>/home/cdcuser/cdc-kafka</cdc-path>
        <cdc-instance>kafka1</cdc-instance>
    </idrcdc-engine>
    <idrcdc-engine name='META' type='Target'>
        <cdc-rsh>ssh cdcuser@zodak</cdc-rsh>
        <cdc-path>/datum/sw/cdc-oracle</cdc-path>
        <cdc-instance>meta</cdc-instance>
    </idrcdc-engine>
    <idrcdc-engine name='DS' type='Target'>
        <cdc-rsh>ssh cdcuser@zodak</cdc-rsh>
        <cdc-path>/datum/sw/cdc-ds</cdc-path>
        <cdc-instance>DS</cdc-instance>
    </idrcdc-engine>
</idrcdc-autosub>
